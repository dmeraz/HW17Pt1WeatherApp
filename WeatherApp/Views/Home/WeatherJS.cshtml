
@{
    ViewBag.Title = "WeatherJS";
}

<h2>WeatherJS</h2>

<p id="name"></p>
<p id="temp"></p>
<p id="humidity"></p>

<script>
    $(document).ready(function () {
        $.get("http://api.openweathermap.org/data/2.5/weather?q=Cleveland&APPID=dcc54514a9fb87e33b7c1ebf436bc9a4&units=imperial", function (response) {
            //response
            console.log(response);
            $("#name").text(response.name);
            $("#temp").text(response.main.temp);
            $("#humidity").text(response.main.humidity);
        });
    });
</script>